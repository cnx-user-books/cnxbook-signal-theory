                <m:mrow>
                  <m:mo>(</m:mo>
                  <m:mi>x</m:mi>
                  <m:mo>,</m:mo>
                  <m:mover accent="true">
                    <m:mi>x</m:mi>
                    <m:mo>˙</m:mo>
                  </m:mover>
                  <m:mo>,</m:mo>
