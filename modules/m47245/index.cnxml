            </m:mtd>
          </m:mtr>
          <m:mtr>
            <m:mtd columnalign="right">
              <m:mrow>
                <m:msub>
                  <m:mi>f</m:mi>
                  <m:mover accent="true">
                    <m:mi>x</m:mi>
